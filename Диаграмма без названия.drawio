        <mxCell id="ckcIEzo2NA76HNStzIgR-4" value="Elastic" style="shape=cylinder3;whiteSpace=wrap;html=1;boundedLbl=1;backgroundOutline=1;size=15;fontSize=7;" vertex="1" parent="WIyWlLk6GJQsqaUBKTNV-1">
